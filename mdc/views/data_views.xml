<odoo>
  <data>

    <!-- Search view: default search for worksheets -->
    <record id="mdc_worksheets_search" model="ir.ui.view">
        <field name="name">mdc.worksheets.search</field>
        <field name="model">mdc.worksheet</field>
        <field name="arch" type="xml">
            <search string="Worksheet">
                <field name="employee_id"/>
                <field name="start_datetime"/>
                <field name="end_datetime"/>
                <field name="lot_id"/>
                <group expand="0" string="Group By">
                    <filter string="Employee" name="groupby_employee" domain="[]" context="{'group_by':'employee_id'}"/>
                    <filter string="Lot" name="groupby_lot" domain="[]" context="{'group_by':'lot_id'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Tree view: default view for worksheets -->
    <record id="mdc_worksheets_tree" model="ir.ui.view">
        <field name="name">mdc.worksheets.tree</field>
        <field name="model">mdc.worksheet</field>
        <field name="arch" type="xml">
            <tree string="Worksheets">
                <field name="employee_id"/>
                <field name="start_datetime"/>
                <field name="end_datetime"/>
                <field name="lot_id"/>
                <field name="workstation_id"/>
            </tree>
        </field>
    </record>

    <!-- Form view: default form for worksheets -->
    <record id="mdc_worksheets_form" model="ir.ui.view">
        <field name="name">mdc.worksheets.form</field>
        <field name="model">mdc.worksheet</field>
        <field name="arch" type="xml">
            <form string="Worksheets">
                <sheet>
                    <group name="gr_worksheet_main_data" string="Worksheet data">
                        <group>
                            <field name="employee_id"/>
                            <field name="lot_id"/>
                        </group>
                        <group>
                            <field name="start_datetime"/>
                            <field name="end_datetime"/>
                        </group>
                        <group>
                            <field name="workstation_id"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!-- Search view: default search for Data_w_INs -->
    <record id="mdc_data_w_ins_search" model="ir.ui.view">
        <field name="name">mdc.data_w_ins.search</field>
        <field name="model">mdc.data_w_in</field>
        <field name="arch" type="xml">
            <search string="Data_w_in">
                <field name="line_id"/>
                <field name="lot_id"/>
                <group expand="0" string="Group By">
                    <filter string="Line" name="groupby_line" domain="[]" context="{'group_by':'line_id'}"/>
                    <filter string="Lot" name="groupby_lot" domain="[]" context="{'group_by':'lot_id'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Tree view: default view for Data_w_INs -->
    <record id="mdc_data_w_ins_tree" model="ir.ui.view">
        <field name="name">mdc.data_w_ins.tree</field>
        <field name="model">mdc.data_w_in</field>
        <field name="arch" type="xml">
            <tree string="Data_w_ins">
                <field name="line_id"/>
                <field name="lot_id"/>
                <field name="create_datetime"/>
                <field name="weight"/>
                <field name="w_uom_id"/>
                <field name="card_id"/>
                <field name="w_out_id"/>
            </tree>
        </field>
    </record>

    <!-- Form view: default form for Data_w_INs -->
    <record id="mdc_data_w_ins_form" model="ir.ui.view">
        <field name="name">mdc.data_w_ins.form</field>
        <field name="model">mdc.data_w_in</field>
        <field name="arch" type="xml">
            <form string="Data_w_ins">
                <sheet>
                </sheet>
            </form>
        </field>
    </record>


    <!-- Search view: default search for Data_w_OUTs -->
    <record id="mdc_data_w_outs_search" model="ir.ui.view">
        <field name="name">mdc.data_w_outs.search</field>
        <field name="model">mdc.data_w_out</field>
        <field name="arch" type="xml">
            <search string="Data_w_OUT">
                <field name="line_id"/>
                <field name="lot_id"/>
                <field name="shift_id"/>
                <field name="workstation_id"/>
                <field name="employee_id"/>
                <group expand="0" string="Group By">
                    <filter string="Line" name="groupby_line" domain="[]" context="{'group_by':'line_id'}"/>
                    <filter string="Lot" name="groupby_lot" domain="[]" context="{'group_by':'lot_id'}"/>
                    <filter string="Employee" name="groupby_employee" domain="[]" context="{'group_by':'employee_id'}"/>
                </group>
            </search>
        </field>
    </record>

    <!-- Tree view: default view for Data_w_OUTs -->
    <record id="mdc_data_w_outs_tree" model="ir.ui.view">
        <field name="name">mdc.data_w_outs.tree</field>
        <field name="model">mdc.data_w_out</field>
        <field name="arch" type="xml">
            <tree string="Data_w_outs">
                <field name="line_id"/>
                <field name="lot_id"/>
                <field name="create_datetime"/>
                <field name="weight"/>
                <field name="w_uom_id"/>
                <field name="quality_id"/>
                <field name="shift_id"/>
                <field name="workstation_id"/>
                <field name="employee_id"/>
                <field name="shared"/>
                <field name="w_out_categ_id"/>
            </tree>
        </field>
    </record>

    <!-- Form view: default form for Data_w_OUTs -->
    <record id="mdc_data_w_outs_form" model="ir.ui.view">
        <field name="name">mdc.data_w_outs.form</field>
        <field name="model">mdc.data_w_out</field>
        <field name="arch" type="xml">
            <form string="Data_w_outs">
                <sheet>
                </sheet>
            </form>
        </field>
    </record>


  </data>
</odoo>